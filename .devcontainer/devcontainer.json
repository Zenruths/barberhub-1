{
  "name": "BarberHub Full Stack",
  "dockerComposeFile": "docker-compose.yml",
  "service": "api",
  "workspaceFolder": "/app",
  "features": {},
  "forwardPorts": [3000, 80],
  "postCreateCommand": "npm install --prefix /app/backend && npm install --prefix /app/frontend",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "vue.volar",
        "bradlc.vscode-tailwindcss",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "remoteUser": "node"
}